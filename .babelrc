{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions", "safari >= 7"]
        },
        "include": [
          "transform-es2015-arrow-functions",
          "transform-class-properties",
          "transform-object-rest-spread"
        ]
      }
    ],
    [
      "latest",
      {
        "es2015": {
          "modules": false,
          "loose": true
        }
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "components": "./components",
          "modules": "./modules",
          "utils": "./utils"
        }
      }
    ]
  ]
}
